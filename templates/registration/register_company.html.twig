{# {% extends 'base_formulaire_inscription_etudiant.html.twig' %} #}
{% extends 'base.html.twig' %}

{% block head %}
    

    <title>Adopteunetudiant</title>
{% endblock %}
{% block styleregister %}
<link href="{{ asset('assets/css/formulaire.css') }}" rel="stylesheet">
{% endblock %}
{# {% block title %}Register{% endblock %} #}

{% block body %}
<div class="form-register">
<div class="form-inscription">
			<div class="headersubscribe">
				<h2>Inscription</h2>
				<h3><img src="{{ asset('assets/svg/icon-img-etudiant.svg') }}" alt="upload-img-student"></h3>
		</div>
	<!-- form begin  -->
	{{ form_start(registrationForm) }}
	
	<div class="form-group">
		{{ form_row(registrationForm.logo,
        {
            'label-att':{'class':'form-label'}
        }) }}
	</div>

	<div class="form-group">
		{{ form_row(registrationForm.name,
        {
            'label-att':{'class':"form-label"}
        }) }}
 	</div>

	<div class="form-group">
		{{ form_row(registrationForm.city,
        {
            'label-att':{'class':"form-label"}
        }) }}
 	</div>

	<div class="form-group">
		{{ form_row(registrationForm.description,
        {
            'label-att':{'class':"form-label"}
        }) }}
 	</div>

	<div class="form-group">
		{{ form_row(registrationForm.email) }}
	</div>

	<div class="form-group">
		{{ form_row(registrationForm.plainPassword) }}
	</div>

	<div class="form-group submit-subscribe-button-div">
		<button type="submit" class="form-control form-control-lg submit-subscribe-button">Envoi</button>
	</div>

	{{ form_end(registrationForm) }}
{# {{ form(registrationForm) }} #}
</div>
{% endblock %}
